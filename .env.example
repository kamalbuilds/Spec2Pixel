# BRIA API Configuration
BRIA_API_TOKEN=your_api_token_here
BRIA_API_URL=https://engine.prod.bria-api.com/v2

# Optional Configuration
MAX_RETRIES=3
RETRY_DELAY=5
POLLING_INTERVAL=5
REQUEST_TIMEOUT=300

# Feature Flags
ENABLE_HDR=false
ENABLE_16BIT=false
ENABLE_BATCH_PROCESSING=true
MAX_BATCH_SIZE=5

# Logging
LOG_LEVEL=INFO
LOG_FILE=pipeline.log